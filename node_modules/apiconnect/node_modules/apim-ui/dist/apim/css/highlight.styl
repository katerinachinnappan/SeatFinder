    overflow-x: auto
    padding: .5em

  .hljs-formula
    background-color: #eee
    font-style: italic

  .hljs-addition
    background-color: #baeeba

  .hljs-deletion
    background-color: #ffc8bd

  .hljs-emphasis
    font-style: italic

  .hljs-comment
