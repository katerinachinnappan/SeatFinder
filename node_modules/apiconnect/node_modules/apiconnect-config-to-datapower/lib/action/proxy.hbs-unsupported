{{#if password}}
password-alias {{password._name}}
  reset
  password {{password.password}}
exit
{{/if}}

assembly-invoke {{_name}}
  reset
  {{#if title}}summary "{{{-add-escape title}}}"{{/if}}
  url "{{target-url}}"
  method {{#if (and verb (ne verb 'keep'))}}{{verb}}{{else}}Keep{{/if}}
  {{#if tls-profile}}ssl-client {{tls-profile}}{{/if}}
  {{#if timeout}}timeout {{timeout}}{{/if}}
  {{#if username}}username {{username}}{{/if}}
  {{#if password}}password-alias {{password._name}}{{/if}}
  compression {{-onoff compression}}
  cache-type {{#if cache-response}}{{-dash-to-upper-camel cache-response}}{{else}}Protocol{{/if}}
  {{#if (and cache-ttl
          (eq cache-type 'time-to-live'))}}
  ttl {{cache-ttl}}{{/if}}
  {{#if cache-key}}cache-key {{cache-key}}{{/if}}
  {{#if (ne (-join '+' stop-on-error) '')}}
  stop-on-error true
  error-types {{-join '+' stop-on-error}}
  {{/if}}
  {{#if output}}output {{output}}{{/if}}
exit
