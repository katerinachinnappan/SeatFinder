assembly-jwt-validate {{_name}}
  reset
  {{#if title}}summary "{{{-add-escape title}}}"{{/if}}
  jwt "{{#if jwt}}{{jwt}}{{else}}request.headers.authorization{{/if}}"
  output-claims "{{#if output-claims}}{{output-claims}}{{else}}decoded.claims{{/if}}"
  {{#if iss-claim}}iss-claim "{{iss-claim}}"{{/if}}
  {{#if aud-claim}}aud-claim "{{aud-claim}}"{{/if}}
  {{#if jwe-crypto}}jwe-crypto "{{jwe-crypto}}"{{/if}}
  {{#if jwe-jwk}}jwe-jwk "{{jwe-jwk}}"{{/if}}
  {{#if jws-crypto}}jws-crypto "{{jws-crypto}}"{{/if}}
  {{#if jws-jwk}}jws-jwk "{{jws-jwk}}"{{/if}}
exit
