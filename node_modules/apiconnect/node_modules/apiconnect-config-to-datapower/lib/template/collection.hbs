{{#each subscriptions}}
api-subscription {{_name}}
  reset
  {{#each plans}}
  plan {{_name}}
  {{/each}}
exit

{{/each}}

api-collection {{_name}}
  reset
  routing-prefix uri "{{routingPrefix}}"
  processing-rule default-api-rule
  default-rate-limit "100" "1" "hour" "off"
  {{#each plans}}
  plan {{_name}}
  {{/each}}
  {{#each subscriptions}}
  subscriber {{subscriber.id}} "{{{subscriber.hashedSecret}}}" "{{subscriber.name}}" {{_name}} {{#if subscriber.oauthClientType}}{{subscriber.oauthClientType}}{{else}}confidential{{/if}}{{#if subscriber.oauthRedirectionUri}} "{{subscriber.oauthRedirectionUri}}"{{/if}}
  {{/each}}
exit
